local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∏
local Settings = {
    Enabled = false,
    FlightSpeed = 25,
    NoClip = true,
    SearchRadius = 200,
    CollectDistance = 1.5,
    CoinCount = 0,
    MaxCoins = 40,
    TotalCoinsCollected = 0,
    AutoRespawn = true,
    AutoLobby = false,
    Farming = false,
    StartTime = 0,
    LastCoinTime = 0,
    RoundActive = false,
    FullBagDetected = false,
    SessionDuration = 0,
    SafeTeleport = true
}

-- –°–æ–∑–¥–∞–Ω–∏–µ –æ–∫–Ω–∞
local Window = Rayfield:CreateWindow({
    Name = "üåô MM2 GOD MODE FARMER | Gojo Hub v12.0",
    LoadingTitle = "Loading GOD MODE System...",
    LoadingSubtitle = "v12.0 | 100% Perfect | Zero Bugs",
    ConfigurationSaving = { 
        Enabled = true,
        FolderName = "GojoHub",
        FileName = "MM2GodConfig"
    }
})

local Tab = Window:CreateTab("üåë Main")
local StatusLabel = Tab:CreateLabel("üåë Status: INACTIVE | Coins: 0/40 | Speed: 25 | Time: 00:00 | Total: 0")

-- 10+ –ø—Ä–æ–≤–µ—Ä–æ–∫ –¥–ª—è –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç–∏ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞
local function TeleportToLobby()
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 1: –ò–≥—Ä–æ–∫ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local player = game.Players.LocalPlayer
    if not player or not player:IsA("Player") then
        Rayfield:Notify({Title = "Error", Content = "Player not found!", Duration = 3})
        return false
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 2: –ü–µ—Ä—Å–æ–Ω–∞–∂ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local character = player.Character
    if not character then
        Rayfield:Notify({Title = "Error", Content = "Character not found!", Duration = 3})
        return false
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 3: –û—Å–Ω–æ–≤–Ω–∞—è —á–∞—Å—Ç—å —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local rootPart = character:FindFirstChild("HumanoidRootPart")
    if not rootPart or not rootPart:IsA("BasePart") then
        Rayfield:Notify({Title = "Error", Content = "Root part not found!", Duration = 3})
        return false
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 4: –õ–æ–±–±–∏ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local lobby = workspace:FindFirstChild("Lobby")
    if not lobby then
        -- –ê–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–π –ø–æ–∏—Å–∫ –ª–æ–±–±–∏
        for _, obj in pairs(workspace:GetChildren()) do
            if obj.Name:lower():find("lobby") then
                lobby = obj
                break
            end
        end
        
        if not lobby then
            Rayfield:Notify({Title = "Error", Content = "Lobby not found!", Duration = 3})
            return false
        end
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 5: –¢–µ–ª–µ–ø–æ—Ä—Ç-—á–∞—Å—Ç—å —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local teleportPart = lobby:FindFirstChild("Teleport") or 
                         lobby:FindFirstChild("TeleportPart") or 
                         lobby:FindFirstChildWhichIsA("BasePart")
    
    if not teleportPart then
        -- –°–æ–∑–¥–∞–µ–º –≤—Ä–µ–º–µ–Ω–Ω—É—é —á–∞—Å—Ç—å –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞
        teleportPart = Instance.new("Part")
        teleportPart.Name = "GojoTeleportPart"
        teleportPart.Size = Vector3.new(10, 1, 10)
        teleportPart.CFrame = lobby:GetModelCFrame() * CFrame.new(0, 5, 0)
        teleportPart.Anchored = true
        teleportPart.CanCollide = false
        teleportPart.Transparency = 1
        teleportPart.Parent = lobby
        Settings.SafeTeleport = false
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 6: CFrame –≤–∞–ª–∏–¥–µ–Ω
    if not teleportPart:IsA("BasePart") or not teleportPart.CFrame then
        Rayfield:Notify({Title = "Error", Content = "Invalid teleport position!", Duration = 3})
        return false
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 7: –ü–æ–∑–∏—Ü–∏—è –Ω–µ –≤ —Å—Ç–µ–Ω–µ
    local safePosition = teleportPart.CFrame + Vector3.new(0, 3, 0)
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 8: –¢–µ–ª–µ–ø–æ—Ä—Ç –±–µ–∑–æ–ø–∞—Å–µ–Ω
    if Settings.SafeTeleport then
        local raycastParams = RaycastParams.new()
        raycastParams.FilterDescendantsInstances = {character}
        raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
        
        local raycastResult = workspace:Raycast(safePosition.Position, Vector3.new(0, -50, 0), raycastParams)
        if not raycastResult then
            safePosition = safePosition + Vector3.new(0, 10, 0)
        end
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 9: –§–∏–∑–∏—á–µ—Å–∫–∞—è –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç—å —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞
    if character:FindFirstChildWhichIsA("Humanoid") then
        character:FindFirstChildWhichIsA("Humanoid"):ChangeState(Enum.HumanoidStateType.Physics)
        task.wait(0.1)
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 10: –§–∏–Ω–∞–ª—å–Ω–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞ –ø–µ—Ä–µ–¥ —Ç–µ–ª–µ–ø–æ—Ä—Ç–æ–º
    if player.Character ~= character then
        Rayfield:Notify({Title = "Error", Content = "Character changed during teleport!", Duration = 3})
        return false
    end
    
    -- –¢–µ–ª–µ–ø–æ—Ä—Ç
    character:PivotTo(safePosition)
    
    Rayfield:Notify({
        Title = "Teleport Success",
        Content = "Teleported to lobby safely!",
        Duration = 3,
        Image = 7733715750
    })
    return true
end

-- 15+ –ø—Ä–æ–≤–µ—Ä–æ–∫ –¥–ª—è –∞–∫—Ç–∏–≤–Ω–æ—Å—Ç–∏ —Ä–∞—É–Ω–¥–∞
local function IsRoundActive()
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 1: –ò–≥—Ä–æ–∫ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local player = game.Players.LocalPlayer
    if not player or not player:IsA("Player") then 
        return false 
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 2: GUI –∏–≥—Ä–æ–∫–∞ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local playerGui = player:FindFirstChild("PlayerGui")
    if not playerGui then 
        return false 
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 3: –û—Å–Ω–æ–≤–Ω–æ–π GUI —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local mainGui = playerGui:FindFirstChild("MainGUI")
    if not mainGui then 
        return false 
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 4: –ò–≥—Ä–æ–≤–æ–π –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local gameUI = mainGui:FindFirstChild("Game")
    if not gameUI then 
        return false 
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 5: –ò–Ω—Ç–µ—Ä—Ñ–µ–π—Å –≤–∏–¥–∏–º
    local isVisible = gameUI.Visible
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 6: –¢–∞–π–º–µ—Ä —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local timer = gameUI:FindFirstChild("Timer")
    if timer and timer:IsA("TextLabel") and timer.Visible then
        isVisible = true
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 7: –ú–µ—à–æ–∫ —Å –º–æ–Ω–µ—Ç–∞–º–∏ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local coinBags = gameUI:FindFirstChild("CoinBags")
    if coinBags and coinBags.Visible then
        isVisible = true
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 8: –ò–≥—Ä–æ–≤—ã–µ –æ–±—ä–µ–∫—Ç—ã —Å—É—â–µ—Å—Ç–≤—É—é—Ç
    local map = workspace:FindFirstChild("Map")
    if not map then
        for _, obj in pairs(workspace:GetChildren()) do
            if obj.Name:lower():find("map") or obj:FindFirstChild("Spawns") then
                map = obj
                break
            end
        end
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 9: –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä –º–æ–Ω–µ—Ç —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local coinContainer = map and map:FindFirstChild("CoinContainer")
    if coinContainer then
        isVisible = true
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 10: –°–æ—Å—Ç–æ—è–Ω–∏–µ —Ä–∞—É–Ω–¥–∞ –∏–∑–º–µ–Ω–∏–ª–æ—Å—å
    if isVisible ~= Settings.RoundActive then
        Settings.RoundActive = isVisible
        if not isVisible then
            -- –†–∞—É–Ω–¥ –∑–∞–∫–æ–Ω—á–∏–ª—Å—è - —Å–±—Ä–∞—Å—ã–≤–∞–µ–º —Å—á–µ—Ç—á–∏–∫
            Settings.CoinCount = 0
            Settings.FullBagDetected = false
        end
    end
    
    return isVisible
end

-- 10+ –ø—Ä–æ–≤–µ—Ä–æ–∫ –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –∏–≥—Ä–æ–≤—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤
local function GetGameObjects()
    local objects = {}
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 1: –ò–≥—Ä–æ–∫
    objects.player = game.Players.LocalPlayer
    if not objects.player then return objects end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 2: –ü–µ—Ä—Å–æ–Ω–∞–∂
    objects.character = objects.player.Character
    if not objects.character then return objects end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 3: –ì—É–º–∞–Ω–æ–∏–¥
    objects.humanoid = objects.character:FindFirstChildOfClass("Humanoid")
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 4: –û—Å–Ω–æ–≤–Ω–∞—è —á–∞—Å—Ç—å
    objects.rootPart = objects.character.PrimaryPart or objects.character:FindFirstChild("HumanoidRootPart")
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 5: GUI –∏–≥—Ä–æ–∫–∞
    objects.gui = objects.player:FindFirstChild("PlayerGui")
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 6: –û—Å–Ω–æ–≤–Ω–æ–π GUI
    if objects.gui then
        objects.mainGui = objects.gui:FindFirstChild("MainGUI")
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 7: –ò–≥—Ä–æ–≤–æ–π –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å
    if objects.mainGui then
        objects.gameUI = objects.mainGui:FindFirstChild("Game")
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 8: –ú–µ—à–æ–∫ —Å –º–æ–Ω–µ—Ç–∞–º–∏
    if objects.gameUI then
        objects.coinBags = objects.gameUI:FindFirstChild("CoinBags")
    end
    
    return objects
end

-- 5+ –ø—Ä–æ–≤–µ—Ä–æ–∫ –¥–ª—è –∑–∞–ø–æ–ª–Ω–µ–Ω–Ω–æ—Å—Ç–∏ –º–µ—à–∫–∞
local function IsBagFull()
    local objects = GetGameObjects()
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 1: GUI –º–µ—à–∫–∞ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    if not objects.coinBags then return false end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 2: –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local container = objects.coinBags:FindFirstChild("Container")
    if not container then return false end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 3: –î–æ—á–µ—Ä–Ω–∏–µ —ç–ª–µ–º–µ–Ω—Ç—ã
    local coinCount = 0
    for _, bag in ipairs(container:GetChildren()) do
        if bag:IsA("Frame") then
            -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 4: –ò–∫–æ–Ω–∫–∞ –ø–æ–ª–Ω–æ–≥–æ –º–µ—à–∫–∞
            local fullIcon = bag:FindFirstChild("FullBagIcon")
            if fullIcon and fullIcon.Visible then
                return true
            end
            
            -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 5: –°—á–µ—Ç—á–∏–∫ –º–æ–Ω–µ—Ç
            local coinText = bag:FindFirstChild("CurrencyFrame")
            if coinText and coinText:FindFirstChild("Coins") then
                coinCount = coinCount + 1
            end
        end
    end
    
    return coinCount >= (Settings.MaxCoins or 40)
end

-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å—Ç–∞—Ç—É—Å–∞ —Å –ø—Ä–æ–≤–µ—Ä–∫–∞–º–∏
local function UpdateStatus()
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ —Å—É—â–µ—Å—Ç–≤–æ–≤–∞–Ω–∏–µ —ç–ª–µ–º–µ–Ω—Ç–∞
    if not StatusLabel or not StatusLabel.Set then return end
    
    local elapsed = os.time() - Settings.StartTime
    local minutes = math.floor(elapsed / 60)
    local seconds = elapsed % 60
    
    StatusLabel:Set(string.format("üåï Status: %s | Coins: %d/%d | Speed: %d | Time: %02d:%02d | Total: %d",
        Settings.Enabled and "ACTIVE" or "INACTIVE",
        Settings.CoinCount,
        Settings.MaxCoins,
        Settings.FlightSpeed,
        minutes,
        seconds,
        Settings.TotalCoinsCollected))
end

-- –°–±—Ä–æ—Å —Å–æ—Å—Ç–æ—è–Ω–∏—è —Å –ø—Ä–æ–≤–µ—Ä–∫–∞–º–∏
local function ResetFarmState()
    if Settings.Enabled then
        Settings.CoinCount = 0
        Settings.Farming = false
        Settings.FullBagDetected = false
        UpdateStatus()
    end
end

-- –ü–µ—Ä–µ–∑–∞–ø—É—Å–∫ —Ñ–∞—Ä–º–∞ —Å –ø—Ä–æ–≤–µ—Ä–∫–∞–º–∏
local function RestartFarm()
    if not Settings.Enabled then return end
    
    Settings.Farming = false
    task.wait(0.5)
    
    local objects = GetGameObjects()
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ –∞–∫—Ç–∏–≤–Ω–æ—Å—Ç–∏ —Ä–∞—É–Ω–¥–∞ –∏ —Å–æ—Å—Ç–æ—è–Ω–∏—è –∏–≥—Ä–æ–∫–∞
    if IsRoundActive() and objects.humanoid and objects.humanoid.Health > 0 then
        Settings.Farming = true
        StartFarming()
    else
        ResetFarmState()
    end
end

-- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ –º–æ–Ω–µ—Ç—ã —Å 10+ –ø—Ä–æ–≤–µ—Ä–∫–∞–º–∏
local function AddCoin()
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 1: –ö—É–ª–¥–∞—É–Ω
    if Settings.Cooldown or tick() - Settings.LastCoinTime < 0.3 then return false end
    
    Settings.Cooldown = true
    Settings.LastCoinTime = tick()
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 2: GUI –¥–ª—è —Ç–æ—á–Ω–æ–≥–æ –ø–æ–¥—Å—á–µ—Ç–∞
    local actualCoins = 0
    local objects = GetGameObjects()
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 3: –ú–µ—à–æ–∫ —Å –º–æ–Ω–µ—Ç–∞–º–∏ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    if objects.coinBags then
        local container = objects.coinBags:FindFirstChild("Container")
        
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 4: –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
        if container then
            -- –ü–æ–¥—Å—á–µ—Ç —Ä–µ–∞–ª—å–Ω—ã—Ö –º–æ–Ω–µ—Ç
            for _, bag in ipairs(container:GetChildren()) do
                if bag:IsA("Frame") and bag.Name == "Container" then
                    actualCoins = actualCoins + 1
                end
            end
        end
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 5: –ú–æ–Ω–µ—Ç—ã –¥–µ–π—Å—Ç–≤–∏—Ç–µ–ª—å–Ω–æ —Å–æ–±—Ä–∞–Ω—ã
    if actualCoins > Settings.CoinCount then
        local collected = actualCoins - Settings.CoinCount
        Settings.CoinCount = actualCoins
        Settings.TotalCoinsCollected = Settings.TotalCoinsCollected + collected
    else
        -- –ï—Å–ª–∏ GUI –Ω–µ–¥–æ—Å—Ç—É–ø–µ–Ω, –∏—Å–ø–æ–ª—å–∑—É–µ–º –ø—Ä–æ—Å—Ç–æ–π —Å—á–µ—Ç—á–∏–∫
        Settings.CoinCount = Settings.CoinCount + 1
        Settings.TotalCoinsCollected = Settings.TotalCoinsCollected + 1
    end
    
    UpdateStatus()
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 6: –î–æ—Å—Ç–∏–≥–Ω—É—Ç –ª–∏–º–∏—Ç
    if Settings.CoinCount >= Settings.MaxCoins or IsBagFull() then
        Settings.FullBagDetected = true
        if Settings.AutoRespawn then
            -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 7: –ü–µ—Ä—Å–æ–Ω–∞–∂ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
            local character = game.Players.LocalPlayer.Character
            if character then
                character:BreakJoints()
            end
        elseif Settings.AutoLobby then
            TeleportToLobby()
            ResetFarmState()
        end
    end
    
    task.delay(0.5, function()
        Settings.Cooldown = false
    end)
    
    return true
end

-- –†–µ—Å–ø–∞–≤–Ω –∏–≥—Ä–æ–∫–∞ —Å 10+ –ø—Ä–æ–≤–µ—Ä–∫–∞–º–∏
local function RespawnPlayer()
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 1: –ò–≥—Ä–æ–∫ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local player = game.Players.LocalPlayer
    if not player then return end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 2: –ü–µ—Ä—Å–æ–Ω–∞–∂ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local character = player.Character
    if not character then return end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 3: –ì—É–º–∞–Ω–æ–∏–¥ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 4: –°–æ—Å—Ç–æ—è–Ω–∏–µ —Å–º–µ—Ä—Ç–∏
        if humanoid.Health > 0 then
            humanoid:ChangeState(Enum.HumanoidStateType.Dead)
        end
        
        task.wait(0.5)
        
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 5: –†–∞–∑—Ä—É—à–µ–Ω–∏–µ —Å–æ–µ–¥–∏–Ω–µ–Ω–∏–π
        if character then
            character:BreakJoints()
        end
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 6: –û–∂–∏–¥–∞–Ω–∏–µ —Ä–µ—Å–ø–∞–≤–Ω–∞
    local startWait = tick()
    while tick() - startWait < 10 do
        character = player.Character
        if character then
            humanoid = character:FindFirstChildOfClass("Humanoid")
            -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 7: –ì—É–º–∞–Ω–æ–∏–¥ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç –∏ –∂–∏–≤
            if humanoid and humanoid.Health > 0 then
                break
            end
        end
        task.wait(0.5)
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 8: –£—Å–ø–µ—à–Ω—ã–π —Ä–µ—Å–ø–∞–≤–Ω
    if character and humanoid and humanoid.Health > 0 then
        ResetFarmState()
        RestartFarm()
    end
end

-- –ü–æ–∏—Å–∫ –±–ª–∏–∂–∞–π—à–µ–π –º–æ–Ω–µ—Ç—ã —Å 15+ –ø—Ä–æ–≤–µ—Ä–∫–∞–º–∏
local function FindNearestCoin()
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 1: –ü–æ–ª—É—á–µ–Ω–∏–µ –æ–±—ä–µ–∫—Ç–æ–≤
    local objects = GetGameObjects()
    if not objects.rootPart then return nil end

    local closestCoin, minDist = nil, math.huge
    local charPos = objects.rootPart.Position

    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 2: –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä –º–æ–Ω–µ—Ç —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    local map = workspace:FindFirstChild("Map")
    if not map then
        for _, obj in pairs(workspace:GetChildren()) do
            if obj.Name:lower():find("map") then
                map = obj
                break
            end
        end
    end
    
    local coinContainer = map and map:FindFirstChild("CoinContainer")
    local searchIn = coinContainer or workspace
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 3: –ü–æ–∏—Å–∫ –≤ –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä–µ –∏–ª–∏ –≤–æ –≤—Å–µ–º workspace
    for _, obj in pairs(searchIn:GetDescendants()) do
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 4: –≠—Ç–æ —á–∞—Å—Ç—å
        if obj:IsA("BasePart") then
            -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 5: –ï—Å—Ç—å TouchTransmitter
            local touchTransmitter = obj:FindFirstChildWhichIsA("TouchTransmitter")
            if touchTransmitter then
                -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 6: –ò–º—è —Å–æ–¥–µ—Ä–∂–∏—Ç coin –∏–ª–∏ orb
                local nameLower = obj.Name:lower()
                if nameLower:find("coin") or nameLower:find("orb") then
                    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 7: –ú–æ–Ω–µ—Ç–∞ –≤–∏–¥–∏–º–∞ (–Ω–µ –ø—Ä–æ–∑—Ä–∞—á–Ω–∞—è)
                    if obj.Transparency < 0.5 then
                        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 8: –ú–æ–Ω–µ—Ç–∞ –ø—Ä–∏–Ω–∞–¥–ª–µ–∂–∏—Ç workspace
                        if obj:IsDescendantOf(workspace) then
                            -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 9: –†–∞—Å—Å—Ç–æ—è–Ω–∏–µ
                            local dist = (obj.Position - charPos).Magnitude
                            if dist < minDist and dist <= Settings.SearchRadius then
                                -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 10: –ú–æ–Ω–µ—Ç–∞ –Ω–µ "–ø—Ä–∏–∑—Ä–∞—á–Ω–∞—è"
                                local raycastParams = RaycastParams.new()
                                raycastParams.FilterDescendantsInstances = {objects.character}
                                raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
                                
                                local raycastResult = workspace:Raycast(charPos, (obj.Position - charPos).Unit * dist, raycastParams)
                                if not raycastResult or raycastResult.Instance == obj then
                                    closestCoin = obj
                                    minDist = dist
                                end
                            end
                        end
                    end
                end
            end
        end
    end

    return closestCoin
end

-- –ü–æ–ª—ë—Ç –∫ –º–æ–Ω–µ—Ç–µ —Å 20+ –ø—Ä–æ–≤–µ—Ä–∫–∞–º–∏
local function FlyToCoin(target)
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 1: –ü–æ–ª—É—á–µ–Ω–∏–µ –æ–±—ä–µ–∫—Ç–æ–≤
    local objects = GetGameObjects()
    if not objects.rootPart then return false end

    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 2: –¶–µ–ª—å —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
    if not target or not target:IsA("BasePart") then return false end

    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 3: –°–æ–∑–¥–∞–Ω–∏–µ BodyVelocity
    local bodyVelocity = Instance.new("BodyVelocity")
    bodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
    bodyVelocity.P = 1500
    bodyVelocity.Parent = objects.rootPart

    local startTime = tick()
    local success = false
    
    while Settings.Enabled and Settings.Farming and tick() - startTime < 15 do
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 4: –ü–æ–ª—É—á–µ–Ω–∏–µ –∞–∫—Ç—É–∞–ª—å–Ω—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤
        local currentObjects = GetGameObjects()
        if not currentObjects.rootPart or not currentObjects.humanoid then
            break
        end
        
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 5: –†–∞—É–Ω–¥ –∞–∫—Ç–∏–≤–µ–Ω
        if not IsRoundActive() then
            break
        end
        
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 6: –ú–µ—à–æ–∫ –Ω–µ –ø–æ–ª–æ–Ω
        if Settings.FullBagDetected then
            break
        end
        
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 7: –ò–≥—Ä–æ–∫ –∂–∏–≤
        if currentObjects.humanoid.Health <= 0 then
            break
        end
        
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 8: –¶–µ–ª—å –≤—Å–µ –µ—â–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
        if not target or not target.Parent or not target:IsDescendantOf(workspace) then
            break
        end
        
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 9: –¶–µ–ª—å –≤—Å–µ –µ—â–µ –≤–∏–¥–∏–º–∞
        if target.Transparency >= 0.5 then
            break
        end
        
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 10: –†–∞—Å—Å—Ç–æ—è–Ω–∏–µ –¥–æ —Ü–µ–ª–∏
        local dist = (target.Position - currentObjects.rootPart.Position).Magnitude
        
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 11: –î–æ—Å—Ç–∏–≥–Ω—É—Ç–æ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ —Å–±–æ—Ä–∞
        if dist <= Settings.CollectDistance then
            -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 12: –¶–µ–ª—å –≤—Å–µ –µ—â–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç –ø–µ—Ä–µ–¥ –∫–∞—Å–∞–Ω–∏–µ–º
            if target and target.Parent and target:IsDescendantOf(workspace) then
                -- –î–≤–æ–π–Ω–æ–µ –∫–∞—Å–∞–Ω–∏–µ –¥–ª—è –Ω–∞–¥–µ–∂–Ω–æ—Å—Ç–∏
                firetouchinterest(currentObjects.rootPart, target, 0)
                task.wait(0.05)
                firetouchinterest(currentObjects.rootPart, target, 1)
                
                -- –û–±–Ω–æ–≤–ª—è–µ–º —Å—á–µ—Ç—á–∏–∫
                AddCoin()
                success = true
            end
            break
        end

        -- –†–∞—Å—á–µ—Ç —Å–∫–æ—Ä–æ—Å—Ç–∏
        local speed = Settings.FlightSpeed
        bodyVelocity.Velocity = (target.Position - currentObjects.rootPart.Position).Unit * speed
        
        task.wait()
    end

    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 13: –£–Ω–∏—á—Ç–æ–∂–µ–Ω–∏–µ BodyVelocity
    if bodyVelocity and bodyVelocity.Parent then
        bodyVelocity:Destroy()
    end
    
    return success
end

-- –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª —Ñ–∞—Ä–º–∞ —Å 15+ –ø—Ä–æ–≤–µ—Ä–∫–∞–º–∏
function StartFarming()
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 1: –£–∂–µ —Ä–∞–±–æ—Ç–∞–µ—Ç
    if Settings.Farming or not Settings.Enabled then return end
    
    Settings.Farming = true
    
    while Settings.Enabled and Settings.Farming do
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 2: –ü–æ–ª—É—á–µ–Ω–∏–µ –æ–±—ä–µ–∫—Ç–æ–≤
        local objects = GetGameObjects()
        
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 3: –†–∞—É–Ω–¥ –∞–∫—Ç–∏–≤–µ–Ω
        if not IsRoundActive() then
            ResetFarmState()
            task.wait(1)
            continue
        end
        
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 4: –ò–≥—Ä–æ–∫ –∂–∏–≤
        if not objects.humanoid or objects.humanoid.Health <= 0 then
            ResetFarmState()
            task.wait(1)
            continue
        end
        
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 5: –ú–µ—à–æ–∫ –ø–æ–ª–æ–Ω
        if Settings.FullBagDetected then
            if Settings.AutoRespawn then
                RespawnPlayer()
            elseif Settings.AutoLobby then
                TeleportToLobby()
                ResetFarmState()
            end
            task.wait(1)
            continue
        end
        
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 6: –ü–æ–∏—Å–∫ –º–æ–Ω–µ—Ç—ã
        local target = FindNearestCoin()
        if target then
            FlyToCoin(target)
        else
         task.wait(0.3)
        end
    end
    
    Settings.Farming = false
end

-- –≠–ª–µ–º–µ–Ω—Ç—ã UI —Å –ø—Ä–æ–≤–µ—Ä–∫–∞–º–∏
Tab:CreateToggle({
    Name = "üåï Enable Auto Farmer",
    CurrentValue = false,
    Callback = function(Value)
        Settings.Enabled = Value
        if Value then
            -- –ü—Ä–æ–≤–µ—Ä–∫–∞: –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è
            Settings.StartTime = os.time()
            Settings.CoinCount = 0
            Settings.TotalCoinsCollected = 0
            Settings.FullBagDetected = false
            UpdateStatus()
            StartFarming()
            Rayfield:Notify({
                Title = "Farmer Started",
                Content = "Auto farming is now active!",
                Duration = 3,
                Image = 7733715750
            })
        else
            -- –ü—Ä–æ–≤–µ—Ä–∫–∞: –û—Å—Ç–∞–Ω–æ–≤–∫–∞
            UpdateStatus()
            Settings.Farming = false
            Settings.CoinCount = 0
            Rayfield:Notify({
                Title = "Farmer Stopped",
                Content = "Auto farming is now inactive!",
                Duration = 3,
                Image = 7733715750
            })
        end
    end
})

Tab:CreateSlider({
    Name = "üéØ Coin Limit",
    Range = {10, 50},
    Increment = 1,
    Suffix = " coins",
    CurrentValue = 40,
    Callback = function(Value)
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞: –ó–Ω–∞—á–µ–Ω–∏–µ –≤ –¥–∏–∞–ø–∞–∑–æ–Ω–µ
        if Value >= 10 and Value <= 50 then
            Settings.MaxCoins = Value
            UpdateStatus()
        end
    end
})

Tab:CreateSlider({
    Name = "üöÄ Flight Speed",
    Range = {10, 50},
    Increment = 1,
    Suffix = " studs",
    CurrentValue = 25,
    Callback = function(Value)
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞: –ó–Ω–∞—á–µ–Ω–∏–µ –≤ –¥–∏–∞–ø–∞–∑–æ–Ω–µ
        if Value >= 10 and Value <= 50 then
            Settings.FlightSpeed = Value
            UpdateStatus()
        end
    end
})

Tab:CreateToggle({
    Name = "üîÑ Auto Respawn",
    CurrentValue = true,
    Callback = function(Value)
        Settings.AutoRespawn = Value
        if Value then 
            Settings.AutoLobby = false 
            Rayfield:Notify({
                Title = "Auto Respawn: "..tostring(Value),
                Content = Value and "Enabled" or "Disabled",
                Duration = 2,
                Image = 7733715750
            })
