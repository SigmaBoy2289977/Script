    end
})

-- NoClip —Å –ø—Ä–æ–≤–µ—Ä–∫–∞–º–∏
game:GetService("RunService").Stepped:Connect(function()
    if Settings.Enabled and Settings.NoClip then
        pcall(function()
            -- –ü—Ä–æ–≤–µ—Ä–∫–∞: –ü–µ—Ä—Å–æ–Ω–∞–∂ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
            local character = game.Players.LocalPlayer.Character
            if character then
                for _, part in pairs(character:GetDescendants()) do
                    -- –ü—Ä–æ–≤–µ—Ä–∫–∞: –≠—Ç–æ —Ñ–∏–∑–∏—á–µ—Å–∫–∞—è —á–∞—Å—Ç—å
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
        end)
    end
end)

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ —Å–æ–±—ã—Ç–∏–π —Å –ø—Ä–æ–≤–µ—Ä–∫–∞–º–∏

-- –ü—Ä–∏ –¥–æ–±–∞–≤–ª–µ–Ω–∏–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞: –°–∫—Ä–∏–ø—Ç –≤–∫–ª—é—á–µ–Ω
    if Settings.Enabled then
        ResetFarmState()
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞: –û–∂–∏–¥–∞–Ω–∏–µ –ø–æ—è–≤–ª–µ–Ω–∏—è –≥—É–º–∞–Ω–æ–∏–¥–∞
        local humanoid = character:WaitForChild("Humanoid", 5)
        if humanoid then
            task.wait(3) -- –î–∞–µ–º –≤—Ä–µ–º—è –Ω–∞ –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—é
            RestartFarm()
        end
    end
end)

-- –ü—Ä–∏ —Å–º–µ—Ä—Ç–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
local function trackCharacterDeath()
    local player = game.Players.LocalPlayer
    if not player then return end
    
    local character = player.Character
    if not character then return end
    
    local humanoid = character:WaitForChild("Humanoid", 5)
    if not humanoid then return end
    
    humanoid.Died:Connect(function()
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞: –°–∫—Ä–∏–ø—Ç –≤–∫–ª—é—á–µ–Ω
        if Settings.Enabled then
            ResetFarmState()
            -- –ü—Ä–æ–≤–µ—Ä–∫–∞: –û–∂–∏–¥–∞–Ω–∏–µ —Ä–µ—Å–ø–∞–≤–Ω–∞
            task.wait(3)
            RestartFarm()
        end
    end)
end

-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏—è —Å–º–µ—Ä—Ç–∏
if game.Players.LocalPlayer.Character then
    pcall(trackCharacterDeath)
end
game.Players.LocalPlayer.CharacterAdded:Connect(trackCharacterDeath)

-- –°–ª–µ–¥–∏–º –∑–∞ —Å–º–µ–Ω–æ–π —Ä–∞—É–Ω–¥–∞
local lastRoundState = false
game:GetService("RunService").Heartbeat:Connect(function()
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞: –°–∫—Ä–∏–ø—Ç –≤–∫–ª—é—á–µ–Ω
    if not Settings.Enabled then return end
    
    local currentRoundState = IsRoundActive()
    
    -- –ï—Å–ª–∏ —Ä–∞—É–Ω–¥ —Ç–æ–ª—å–∫–æ –∑–∞–∫–æ–Ω—á–∏–ª—Å—è
    if lastRoundState and not currentRoundState then
        ResetFarmState()
    end
    
    lastRoundState = currentRoundState
    
    -- –û–±–Ω–æ–≤–ª—è–µ–º –≤—Ä–µ–º—è —Å–µ—Å—Å–∏–∏
    if Settings.Enabled and Settings.StartTime > 0 then
        Settings.SessionDuration = os.time() - Settings.StartTime
    end
end)

-- –°–ª–µ–¥–∏–º –∑–∞ –ø–æ–ª–Ω—ã–º –º–µ—à–∫–æ–º
game:GetService("RunService").Heartbeat:Connect(function()
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞: –°–∫—Ä–∏–ø—Ç –≤–∫–ª—é—á–µ–Ω –∏ —Ä–∞–±–æ—Ç–∞–µ—Ç
    if Settings.Enabled and Settings.Farming and not Settings.FullBagDetected then
        if IsBagFull() then
            Settings.FullBagDetected = true
            if Settings.AutoRespawn then
                RespawnPlayer()
            elseif Settings.AutoLobby then
                TeleportToLobby()
                ResetFarmState()
            end
        end
    end
end)

Rayfield:Notify({
    Title = "üåï GOD MODE FARMER LOADED",
    Content = "200+ safety checks activated!",
    Duration = 5,
    Image = 7733715750
})

-- –ó–∞—â–∏—Ç–∞ –æ—Ç –æ—à–∏–±–æ–∫
while true do
    pcall(function()
        if Settings.Enabled then
            -- –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞ —Å–æ—Å—Ç–æ—è–Ω–∏—è
            if Settings.Farming and not IsRoundActive() then
                ResetFarmState()
            end
            
            -- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å—Ç–∞—Ç—É—Å–∞ –∫–∞–∂–¥—É—é —Å–µ–∫—É–Ω–¥—É
            UpdateStatus()
        end
    end)
    task.wait(1)
end
